{
  "name": "BRP",
  "version": "0.0.0",
  "main": "app.js",
  "description": "A Home Office application for Biometric Residence Permit covering lost and stolen, error on card and delivery error services.",
  "license": "GPL-2.0",
  "engines": {
    "node": "^14.15.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "hof-build watch",
    "dev": "NODE_ENV=development hof-build watch",
    "test": "yarn run test:lint && yarn run test:unit",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:unit": "NODE_ENV=test istanbul cover _mocha ./test",
    "test:acceptance": "npm run test:acceptance:new && npm run test:acceptance:old",
    "test:acceptance:new": "funkie so-acceptance --steps",
    "test:acceptance:old": "cd acceptance_tests/; bundle install && cucumber",
    "quality": "plato -r -x 'node_modules|reports|test' -d reports/plato .",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test",
    "build": "hof-build",
    "postinstall": "yarn run build"
  },
  "author": "HomeOffice",
  "dependencies": {
    "express": "^4.17.1",
    "hof": "^19.5.1",
    "hogan.js": "^3.0.2",
    "hot-shots": "^5.9.0",
    "i18n-future": "^1.1.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-business": "^2.0.0",
    "nodemailer": "^6.4.16",
    "typeahead-aria": "^1.0.1",
    "underscore": "^1.13.1",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "debug": "^2.6.9",
    "eslint": "^7.30.0",
    "eslint-config-hof": "^1.1.0",
    "funkie": "0.0.6",
    "funkie-phantom": "0.0.1",
    "hof-util-reqres": "^1.0.1",
    "install": "^0.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.4.0",
    "mocha-multi": "^1.1.3",
    "mocha-sandbox": "^1.0.0",
    "parallelshell": "^1.1.1",
    "plato": "^1.7.0",
    "proxyquire": "^1.5.0",
    "sinomocha": "^0.2.4",
    "sinon": "^1.15.3",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.666.0",
    "so-acceptance": "^4.3.1"
  },
  "resolutions": {
    "underscore": "^1.12.1"
  }
}
